//This is the main class. Many things will be referenced here
package com.james.soulsfragmod;

import net.minecraft.init.Blocks;
import net.minecraftforge.fml.common.Mod;
import net.minecraftforge.fml.common.Mod.EventHandler;
import net.minecraftforge.fml.common.event.FMLInitializationEvent;

//Basic mod info. Be careful when changing the MODID!
@Mod(modid = SoulsFragMod.MODID, version = SoulsFragMod.VERSION)

public class SoulsFragMod
{
    public static final String MODID = "souls_fragmod";
    public static final String VERSION = "1.0";
    
    @EventHandler
    public void init(FMLInitializationEvent event)
    {
        System.out.println("DIRT BLOCK >> "+Blocks.dirt.getUnlocalizedName());
    }
}